/* 页面背景 */
.page-container.data-v-06210029 {
  min-height: 100vh;
  background-color: #f5f5f5;
  padding: 20rpx;
  box-sizing: border-box;
}

/* 卡片容器 */
.card-box.data-v-06210029 {
  background-color: #ffffff;
  border-radius: 24rpx;
  padding: 0 30rpx;
  margin-bottom: 30rpx;
  box-shadow: 0 4rpx 20rpx rgba(0, 0, 0, 0.03);
}

/* 表单行 */
.form-row.data-v-06210029 {
  display: flex;
  align-items: flex-start; /* 顶部对齐 */
  padding: 36rpx 0;
  border-bottom: 1rpx solid #eeeeee;
  /* 地址展示 */
}
.form-row.no-border.data-v-06210029 {
  border-bottom: none;
}
.form-row.tag-row.data-v-06210029 {
  align-items: center;
}
.form-row .label.data-v-06210029 {
  width: 140rpx;
  font-size: 30rpx;
  color: #333;
  font-weight: bold;
  padding-top: 4rpx; /* 微调对齐 */
}
.form-row .input-box.data-v-06210029 {
  flex: 1;
}
.form-row .uni-input.data-v-06210029 {
  flex: 1;
  font-size: 30rpx;
  color: #333;
}
.form-row .placeholder.data-v-06210029 {
  color: #ccc;
  font-size: 28rpx;
}
.form-row .address-display.data-v-06210029 {
  flex: 1;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.form-row .address-display .addr-text.data-v-06210029 {
  flex: 1;
  font-size: 30rpx;
  line-height: 1.4;
  padding-right: 20rpx;
}
.form-row .address-display .icon-arrow.data-v-06210029 {
  color: #ccc;
  font-size: 32rpx;
}

/* 性别选择胶囊 */
.gender-radio.data-v-06210029 {
  display: flex;
  margin-top: 24rpx;
}
.gender-radio .radio-tag.data-v-06210029 {
  padding: 10rpx 40rpx;
  margin-right: 20rpx;
  border: 1rpx solid #e5e5e5;
  border-radius: 40rpx;
  font-size: 26rpx;
  color: #666;
  background: #fff;
}
.gender-radio .radio-tag.active.data-v-06210029 {
  background: rgba(0, 170, 255, 0.1);
  border-color: #00aaff;
  color: #00aaff;
  font-weight: bold;
}

/* 标签列表 */
.tag-list.data-v-06210029 {
  display: flex;
}
.tag-list .tag-item.data-v-06210029 {
  width: 100rpx;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  border: 1rpx solid #e5e5e5;
  border-radius: 8rpx;
  margin-right: 20rpx;
  font-size: 26rpx;
  color: #666;
}
.tag-list .tag-item.active.data-v-06210029 {
  background: #00aaff;
  border-color: #00aaff;
  color: #fff;
}

/* 底部按钮区 */
.footer-btn.data-v-06210029 {
  margin-top: 60rpx;
}
.footer-btn .btn-save.data-v-06210029 {
  background: linear-gradient(90deg, #00c6ff, #007aff);
  color: #fff;
  border-radius: 50rpx;
  font-size: 32rpx;
  height: 90rpx;
  line-height: 90rpx;
  box-shadow: 0 10rpx 20rpx rgba(0, 122, 255, 0.2);
}
.footer-btn .btn-save.data-v-06210029:active {
  opacity: 0.9;
}
.footer-btn .btn-del.data-v-06210029 {
  text-align: center;
  margin-top: 30rpx;
  color: #ff4d4f;
  font-size: 28rpx;
  background: #fff;
  padding: 20rpx;
  border-radius: 50rpx;
}

